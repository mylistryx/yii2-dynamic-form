{
  "name": "mylistryx/yii2-dynamic-form",
  "description": "Yii2 DynamicForm widget with assets",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "yii2-extension",
  "authors": [
    {
      "name": "Sergey Zhukovskiy",
      "email": "mylistryx@gmail.com",
      "homepage": "https://net23.ru"
    }
  ],
  "keywords": [
    "yii2",
    "dynamic form",
    "active form",
    "asset",
    "bundle",
    "free"
  ],
  "version": "1.0.0",
  "readme": "README.md",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "yii\\DynamicForm\\": "src/"
    }
  },
  "require": {
    "php": ">=8.2",
    "mylistryx/yii2-fontawesome-free": "2.0.0",
    "yiisoft/yii2": "~2.0.50",
	"symfony/css-selector": "~7.0.0",
    "symfony/dom-crawler": "~7.0.0"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
